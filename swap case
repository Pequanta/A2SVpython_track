def swap_case(s):
    res = []
    for i in s:
        if i.isupper():
            res.append(i.lower())
        elif i.islower():
            res.append(i.upper())
        else:
            res.append(i)
    return "".join(res) 

if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
